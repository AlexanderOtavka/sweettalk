{
    "name": "sweettalk",
    "displayName": "SweetTalk",
    "description": "SweetTalk language support",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.42.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "sweettalk",
            "aliases": ["SweetTalk", "sweettalk"],
            "extensions": [".sweet"],
